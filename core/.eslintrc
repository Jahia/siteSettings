{
  "extends": "@jahia",
  "plugins": ["json"],
  "parser": "@babel/eslint-parser",
  "rules": {
    "react/require-default-props": "off",
    "react/boolean-prop-naming": ["error", { "rule": "^(is|has)[A-Z]([A-Za-z0-9]?)+"} ]
  },
  "parserOptions": {
         "requireConfigFile": false,
          "babelOptions": {
            "parserOpts": {
              "plugins": ["jsx"]
            }
          }
  }
}
